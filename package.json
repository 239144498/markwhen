    "luxon": "^3.0.1",
    "pinia": "^2.0.16",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/lru-cache": "^7.10.10",
    "@types/luxon": "^3.0.0",
