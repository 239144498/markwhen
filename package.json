    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.8"
